{
  "name": "@shelf/jest-dynamodb",
  "version": "3.2.0",
  "description": "Run your tests using Jest & DynamoDB local",
  "keywords": [
    "dynamodb",
    "dynamodb local",
    "jest",
    "jest environment",
    "jest preset"
  ],
  "repository": "shelfio/jest-dynamodb",
  "license": "MIT",
  "author": {
    "name": "Vlad Holubiev",
    "email": "vlad@shelf.io",
    "url": "shelf.io"
  },
  "files": [
    "environment.js",
    "jest-preset.js",
    "setup.js",
    "teardown.js",
    "wait-for-localhost.js"
  ],
  "scripts": {
    "coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.json --fix",
    "lint:ci": "eslint . --ext .js,.ts,.json",
    "test": "export ENVIRONMENT=local && jest ."
  },
  "lint-staged": {
    "*.{html,md,yml}": [
      "prettier --write"
    ],
    "*.{js,json}": [
      "eslint --fix"
    ]
  },
  "babel": {
    "extends": "@shelf/babel-config/backend"
  },
  "prettier": "@shelf/prettier-config",
  "jest": {
    "preset": "./jest-preset.js"
  },
  "dependencies": {
    "@aws-sdk/client-dynamodb": "3.113.0",
    "@aws-sdk/lib-dynamodb": "3.113.0",
    "@aws-sdk/util-dynamodb": "3.113.0",
    "cwd": "0.10.0",
    "debug": "4.3.4",
    "dynamodb-local": "0.0.31"
  },
  "devDependencies": {
    "@shelf/babel-config": "1.0.2",
    "@shelf/eslint-config": "2.22.0",
    "@shelf/prettier-config": "1.0.0",
    "eslint": "8.20.0",
    "husky": "8.0.1",
    "jest": "28.1.3",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "typescript": "4.7.4"
  },
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  }
}
